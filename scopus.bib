@ARTICLE{Aheleroff2021,
author={Aheleroff, S. and Xu, X. and Zhong, R.Y. and Lu, Y.},
title={Digital Twin as a Service (DTaaS) in Industry 4.0: An Architecture Reference Model},
journal={Advanced Engineering Informatics},
year={2021},
