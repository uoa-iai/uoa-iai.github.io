@ARTICLE{Chand202388,
author={Chand, S. and Lu, Y.},
title={Dual task scheduling strategy for personalized multi-objective optimization of cycle time and fatigue in human-robot collaboration},
journal={Manufacturing Letters},
year={2023},
volume={35},
