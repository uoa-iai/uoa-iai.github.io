               /post/mbie-hrc-workshop/6_hud55c09bc534bc6c85aa923d96e341b58_1636054_0f1e9a142d4b92980f31e0f4e9bf11ba.webp 760w,
               /post/mbie-hrc-workshop/6_hud55c09bc534bc6c85aa923d96e341b58_1636054_1200x1200_fit_q80_h2_lanczos.webp 1200w&#34;
               src=&#34;https://uoa-iai.github.io/post/mbie-hrc-workshop/6_hud55c09bc534bc6c85aa923d96e341b58_1636054_8aaf208a0f0f0058dee2ecefc7233735.webp&#34;
               width=&#34;760&#34;
